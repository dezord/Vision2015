package org.usfirst.frc.team3324.robot;

import edu.wpi.first.wpilibj.*;

public class MetroJS {
	
	static 
	
	public static int getAxis(){

		double output;
		
		if(  ){

		    output = joystick_->GetRawAxis( axis );

	    }
	    else{

	        output = -joystick_->GetRawAxis( axis );

	    }
		
		if( fabs(output) < GamePad::AXIS_DEADBAND ){
			output = 0.0;
		}

		return output;
			
	}
	
	public MetroJS() {
		
	}
}
